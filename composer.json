{
    "name": "israel-nogueira/mysql-orm",
    "description": "ORM simples para PHP e MySQL",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "keywords": [],
    "authors": [
        {
            "name": "Israel Nogueira",
            "email": "israel@feats.com.br"
        }
    ],
    "autoload": {
        "psr-4": {
            "IsraelNogueira\\MysqlOrm\\": "src/"
        }
    },
    "require":{
        "php":">=7.4"
    },
    "scripts": {
        "post-install-cmd": [
            "if [ ! -d ../orm/models ]; then mkdir -p ../orm/models; fi",
            "php -r \"echo 'Orm\\\\Models\\\\';\" >> composer.json",
            "composer dump-autoload"
        ]
    },



    "minimum-stability": "stable"
}